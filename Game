#include <iostream>

using namespace std;
string pass;
string jawaban;
void salah()
{
     system("title GAME OVER");
     system("color 80");
     system("cls");
     system("color 30");
     cout<<endl;
     system("color 10");
     cout<<endl;
     system("color 82");
     cout<<"Game Over"<<endl;
     system("color f0");
     cout<<"Game Over"<<endl;
     system("color 85");
     system("color 30");
     system("color f0");
     system("color 82");
     system("color 80");
     system("color 85");
     cout<<"tekan enter untuk melanjutkan"<<endl;
     system("pause>>null");
return ;
}
void menang()
{
     system("title OOPS, YOU ARE THE WINNER");
     system("color 80");
     system("cls");
     system("color 30");
     cout<<endl;
     system("color 10");
     cout<<endl;
     system("color 82");
     cout<<"OOPS, CONGRATULATION, YOU ARE THE WINNER"<<endl;
     system("color f0");
     system("color 85");
     system("color 30");
     system("color f0");
     system("color 82");
     system("color 80");
     system("color 85");
     cout<<endl;
     cout<<endl;
     cout<<endl;
     cout<<"ANDA SECARA RESMI TELAH MENAMATKAN GAME INI"<<endl;
     cout<<"SARAN, KRITIK, UMPATAN, KATA BIJAK, BATA, CENDOL dan sebangainya, Kirim ke @Hacksetyobudi | Fb.com/Hacksetyobudi"<<endl;
}
void soal100()
{
     system("title YOU ARE NOT THE WINNER");
     system("color 80");
     system("cls");
     system("color 30");
     cout<<endl;
     system("color 10");
     cout<<endl;
     system("color 82");
     cout<<"OOPS, CONGRATULATION, YOU ARE THE WINNER"<<endl;
     system("color f0");
     cout<<"YEAH, SORRY, YOU ARE GAME OVER"<<endl;
     system("color 85");
     system("color 30");
     system("color f0");
     system("color 82");
     system("color 80");
     system("color 85");
     cout<<" - .... . / -.-. .-. . .- - --- .-. "<<endl;
     cout<<" .- -.-. .-. --- -. -.-- "<<endl;
     cin>>jawaban;
     if (jawaban=="mhs")
     {menang();}
     else
     salah();
}
